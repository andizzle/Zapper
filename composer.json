{
    "name": "andizzle/zapper",
    "description": "A test package that allows you do db envolved testing properly.",
    "keywords": ["laravel", "laravel4", "phpunit", "test"],
    "authors": [
        {
            "name": "Andy Zhang",
            "email": "andizzle.zhang@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "illuminate/support": "4.0.x",
        "illuminate/console": "4.1.x",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Andizzle\\Zapper": "src/"
        }
    },
    "minimum-stability": "dev"
}
